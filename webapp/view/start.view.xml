		<pages><Page title="Bestellungen aus Datei generieren very dirty">
				<content>
				    <sap.ui.layout:VerticalLayout xmlns:sap.ui.layout="sap.ui.layout" width="100%" id="__layout1">
				        <sap.ui.layout:content>
				            <sap.ui.unified:FileUploader xmlns:sap.ui.unified="sap.ui.unified" id="uploader" uploadComplete="handleUploadComplete" uploadUrl="upload/"/>
